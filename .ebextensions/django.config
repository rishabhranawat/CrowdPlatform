container_commands: 
  01_install_mysqldb: 
    command: "pip install distribute==0.6.28; pip install mysql-python;"
  02_syncdb: 
    command: "python manage.py syncdb --noinput"
    leader_only: true
  04_collectstatic: 
    command: "python manage.py collectstatic --noinput"
  05_migrate_crowdplatform: 
    command: "python manage.py migrate create_lesson_plan --noinput"
    leader_only: true
option_settings: 
  - namespace:   "aws:elasticbeanstalk:application:environment"
    option_name: DJANGO_SETTINGS_MODULE
    value: yesua1.settings
    "PYTHONPATH": "/home/goyalshristi/Desktop/CrowdPlatform:$PYTHONPATH"
  - namespace: "aws:elasticbeanstalk:container:python"
    option_name: WSGIPath
    value: yesua1/wsgi.py
  - namespace: "aws:elasticbeanstalk:container:python:staticfiles"
    option_name: /static/
    value: yesua1/static/